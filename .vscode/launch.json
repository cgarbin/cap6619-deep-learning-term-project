{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File (Integrated Terminal)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "cwd": "",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: MNIST DNN Dropout)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "cwd": "",
      "args": [
        "--network",
        "standard",
        "--optimizer",
        "sgd",
        "--hidden_layers",
        "1",
        "--units_per_layer",
        "512",
        "--epochs",
        "5",
        "--batch_size",
        "128",
        "--dropout_rate_input_layer",
        "0.1",
        "--dropout_rate_hidden_layer",
        "0.5",
        "--learning_rate",
        "0.1",
        "--momentum",
        "0.95",
        "--max_norm_max_value",
        "2"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Python: MNIST DNN Batch Normalization)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "cwd": "",
      "args": [
        "--hidden_layers",
        "1",
        "--units_per_layer",
        "512",
        "--epochs",
        "5",
        "--batch_size",
        "128",
        "--optimizer",
        "sgd",
        "--learning_rate",
        "0.1"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Attach",
      "type": "python",
      "request": "attach",
      "port": 5678,
      "host": "localhost"
    },
    {
      "name": "Python: Module",
      "type": "python",
      "request": "launch",
      "module": "enter-your-module-name-here",
      "cwd": "",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "console": "integratedTerminal",
      "cwd": "",
      "args": ["runserver", "--noreload", "--nothreading"],
      "django": true
    },
    {
      "name": "Python: Flask",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "app.py"
      },
      "args": ["run", "--no-debugger", "--no-reload"],
      "jinja": true
    },
    {
      "name": "Python: Current File (External Terminal)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "externalTerminal",
      "cwd": ""
    }
  ]
}
